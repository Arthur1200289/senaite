<tal:contentviews tal:define="ploneview context/@@plone;"
                  tal:condition="ploneview/showToolbar"
                  i18n:domain="plone">

  <tal:block define="tabset1 view/tabSet1;
                     tabset2 view/tabSet2;">
    <ul class="nav nav-pills col-md-12" i18n:domain="plone">
      <tal:actions repeat="action tabset1">
        <li tal:define="selected action/selected|nothing;"
            tal:attributes="id string:contentview-${action/id};
                        class python:selected and 'active' or ''">
          <a href=""
             tal:content="action/title"
             tal:attributes="href action/url;
                   target action/link_target|nothing;"
             i18n:translate="">
            View name
          </a>
        </li>
      </tal:actions>
      <tal:actions repeat="action tabset2">
        <li tal:define="selected action/selected|nothing;"
            tal:attributes="id string:contentview-${action/id};
                        class python:selected and 'active' or 'plain'">
          <a href=""
             tal:content="action/title"
             tal:attributes="href action/url;
                   target action/link_target|nothing;"
             i18n:translate="">
            View name
          </a>
        </li>
      </tal:actions>
    </ul>
  </tal:block>

</tal:contentviews>
